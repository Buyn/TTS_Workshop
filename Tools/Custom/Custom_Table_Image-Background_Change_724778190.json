      "Sticky": true,
      "Tooltip": true,
      "GridProjection": false,
      "XmlUI": "",
      "LuaScript": "function onLoad(save_state)\r\n    -- Set Description if loading from Save different than workshop\r\n    self.setDescription('To add new backgrounds open the Notebook\\nand edit the tab \\\"Backgrounds\\\" with the format:\\n\\n[Title](URL)\\n\\nAnd click the reload button on the top right of this notecard.\\n\\nRed = Error | Green = Ready | Blue = Current')\r\n    -- clearURLs if coming from a Save different than workshop\r\n    clearURLs()\r\n\r\n    -- Create Scanner to find table object\r\n    local scanner = spawnObject({\r\n      ['type'] = 'Metal Ball',\r\n      ['position'] = {0,-30,0}\r\n    })\r\n    --scanner.lock() -- Debug position of the scanner spawn\r\n    -- The scanner will collide with the table and set a variable with the object I can access then self-destruct\r\n    scanner.setLuaScript(\r\n    [[function onCollisionEnter(collision_info)\r\n        if collision_info['collision_object'].name == 'Table_Custom' then\r\n            getObjectFromGUID('ImageChanger').setVar('nativeTable',collision_info['collision_object'])\r\n            --print(\"Table Scan Complete\") -- Debug for success\r\n        else\r\n            print(\"ERROR when scanning Table, retry!\")\r\n        end\r\n        self.destruct()\r\n      end\r\n    ]])\r\n\r\n  unicode = 'â†»' -- This is the reload Unicode Character. Will turn into '?' if seen. He's shy. Also I'm a quantum physicist\r\n  if unicode == '?' then\r\n    addButton({'URL1',700,700,500,'@','reloadURLs',self,{0.5,0,1.7},{0,180,0}})\r\n  else\r\n    addButton({'URL1',700,700,500,unicode,'reloadURLs',self,{0.5,0,1.7},{0,0,0}})\r\n  end\r\n  -- current variable allows us to check what URL was the previous and change the color abck to green\r\n  current = nil\r\nend\r\n\r\n-- Creates a button with a table of params: GUID, Width, Height, Font Size, Label, Function, Function Owner, pos?, rot?\r\nfunction addButton(paramsTable)\r\n    object = getObjectFromGUID(paramsTable[1])\r\n\r\n    position = object.getTable('pos') or paramsTable[8] or {0,0,0}\r\n    rotation = object.getTable('rot') or paramsTable[9] or {0,0,0}\r\n    object.createButton({\r\n        ['position'] = position,\r\n        ['rotation'] = rotation,\r\n        ['width'] = paramsTable[2],\r\n        ['height'] = paramsTable[3],\r\n        ['font_size'] = paramsTable[4],\r\n        ['label'] = paramsTable[5],\r\n        ['click_function'] = paramsTable[6],\r\n        ['function_owner'] = paramsTable[7]\r\n    })\r\nend\r\n\r\n-- Resets eveything to first state\r\nfunction clearURLs()\r\n  for i=1,10 do\r\n    local indicator = getObjectFromGUID(\"URL\"..i)\r\n    indicator.setColorTint({0,0,0})\r\n    -- Since the first indicator has 2 buttons, in that case remove index 2.\r\n    if i == 1 then indicator.removeButton(1) else indicator.clearButtons() end\r\n  end\r\nend\r\n\r\n-- Checks Notebook, parses it and creates buttons accordingly\r\nfunction reloadURLs(obj,player)\r\n  self.setDescription(\"\")\r\n  clearURLs()\r\n  local notebookTable = getNotebookTabs()\r\n  for _,value in pairs(notebookTable) do\r\n    if value['title'] == 'Backgrounds' then\r\n      NotebookString = value['body'] -- Find the first notebook tab with name Backgrounds\r\n      break -- Disregard all others\r\n    end\r\n    if value['index'] == #notebookTable-1 then -- if we get to the last item means \"Backgrounds\" doesn't exit.\r\n      print(\"Error Notebook Tab \\\"Backgrounds\\\" not found. Creating default.\")\r\n      NotebookString = [=[[Gravel](http://2.bp.blogspot.com/-6lIml62O9IM/U9CK_P9zjDI/AAAAAAAAFqs/-8m-7oi9nmU/s1600/Concrete+ground+floor+walkway+pathway+texture+ver+1.jpg)\r\n[Wood](http://bgfons.com/upload/wood_texture3847.jpg)\r\n[Water](https://phenomenex.files.wordpress.com/2016/01/water.jpg)\r\n[Grass](http://www.myfreetextures.com/wp-content/uploads/2015/01/lots-of-grass-background.jpg)\r\n[Blank](http://i.imgur.com/ove85gM.png)]=]\r\n      addNotebookTab({\r\n          ['title'] = 'Backgrounds',\r\n          ['color'] = 'White',\r\n          ['body'] = NotebookString\r\n      })\r\n    end\r\n  end\r\n  local k = 0\r\n  for count=1,10 do\r\n    local statusIndicator = getObjectFromGUID('URL'..count)\r\n    i,k,title,address = string.find(NotebookString, \"(%[.-%])(%(.-%))\",k+1) -- Find the format\r\n    if i == nil then -- if it was the last url\r\n      if string.len(NotebookString) ~= lastCount then -- valida for errors\r\n        statusIndicator.setColorTint({1,0,0})\r\n      end\r\n      break -- stop coloring\r\n    else statusIndicator.setColorTint({0,0.8,0}) end -- if it wasn't color green\r\n    -- Set this button's address and create it's button\r\n    statusIndicator.setVar('address', string.sub(address,2,-2))\r\n    addButton({'URL'..count,10000,500,500,string.sub(title,2,-2),'changeImage',self,{12,0,0},{0,180,0}})\r\n    -- Count the amount of chars to validate for errors\r\n    lastCount = k\r\n  end\r\nend\r\n\r\n-- This is the magic function.\r\nfunction changeImage(obj,player)\r\n  -- if its the first change, sets the previous to current. If not does nothing.\r\n  current = current or obj.getGUID()\r\n  -- Tints previous to green and current to blue\r\n  getObjectFromGUID(current).setColorTint({0,0.8,0})\r\n  obj.setColorTint({0,0,0.8})\r\n\r\n  -- Magic! copy the Custom table, destroy it then paste it with the new URL\r\n  copy({nativeTable})\r\n  nativeTable.destruct()\r\n  nativeTable = paste({\r\n      ['position'] = {0,0,0} -- Paste it exactly where you found it.\r\n  })[1]\r\n  nativeTable.setCustomObject({\r\n      ['image'] = obj.getVar('address') -- the URL is given by the indicator's variable\r\n  })\r\n  current = obj.getGUID() -- set current as previous\r\nend\r\n",
