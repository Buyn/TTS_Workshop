      "LuaScript": "local colInfo = {}\r\ncolObj = nil\r\nsource = nil\r\ntarget = nil\r\ncolText = \"[b]no collision registered[/b]\"\r\n\r\nfunction onLoad()\r\n    self.createButton({\r\n        click_function = \"xplus\",\r\n        function_owner = self,\r\n        label = \"X+\",\r\n        font_size = 40,\r\n        width = 60,\r\n        height = 60,\r\n        position = {0.4, 4.5, -0.4},\r\n        rotation = {0, 180, 0}\r\n        }) \r\n    self.createButton({\r\n        click_function = \"xminus\",\r\n        function_owner = self,\r\n        label = \"X-\",\r\n        font_size = 40,\r\n        width = 60,\r\n        height = 60,\r\n        position = {0.3, 4.5, -0.4},\r\n        rotation = {0, 180, 0}\r\n        })\r\n    self.createButton({\r\n        click_function = \"yplus\",\r\n        function_owner = self,\r\n        label = \"Y+\",\r\n        font_size = 40,\r\n        width = 60,\r\n        height = 60,\r\n        position = {0.2, 4.5, -0.4},\r\n        rotation = {0, 180, 0}\r\n        }) \r\n    self.createButton({\r\n        click_function = \"yminus\",\r\n        function_owner = self,\r\n        label = \"Y-\",\r\n        font_size = 40,\r\n        width = 60,\r\n        height = 60,\r\n        position = {0.1, 4.5, -0.4},\r\n        rotation = {0, 180, 0}\r\n        })\r\n    self.createButton({\r\n        click_function = \"zplus\",\r\n        function_owner = self,\r\n        label = \"Z+\",\r\n        font_size = 40,\r\n        width = 60,\r\n        height = 60,\r\n        position = {0, 4.5, -0.4},\r\n        rotation = {0, 180, 0}\r\n        }) \r\n    self.createButton({\r\n        click_function = \"zminus\",\r\n        function_owner = self,\r\n        label = \"Z-\",\r\n        font_size = 40,\r\n        width = 60,\r\n        height = 60,\r\n        position = {-0.1, 4.5, -0.4},\r\n        rotation = {0, 180, 0}\r\n        }) \r\nend\r\n\r\nfunction onCollisionEnter(colInfo)\r\n    colText = \"[b]collision registered:[/b]\\n\"\r\n    colObj = colInfo.collision_object\r\n    if colObj ~= nil then\r\n        a = colObj.getScale()\r\n        colText = colText .. \"Name: \" .. colObj.getName() .. \" Type: \" .. colObj.tag .. \"\\n X:\" .. a['x'] .. \"\\n Y:\" .. a['y'] .. \"\\n Z:\" .. a['z']\r\n    end\r\n    updateDesc()\r\nend\r\n\r\nfunction xplus()\r\n    if colObj~=nil then\r\n        a = colObj.getScale()\r\n        colObj.scale({1.05, 1, 1})\r\n    else\r\n    end\r\nend\r\n\r\nfunction xminus()\r\n    if colObj~=nil then\r\n        a = colObj.getScale()\r\n        colObj.scale({0.95, 1, 1})\r\n    else\r\n    end\r\nend\r\n\r\nfunction yplus()\r\n    if colObj~=nil then\r\n        a = colObj.getScale()\r\n        colObj.scale({1, 1.05, 1})\r\n    else\r\n    end\r\nend\r\n\r\nfunction yminus()\r\n    if colObj~=nil then\r\n        a = colObj.getScale()\r\n        colObj.scale({1, 0.95, 1})\r\n    else\r\n    end\r\nend\r\n\r\nfunction zplus()\r\n    if colObj~=nil then\r\n        a = colObj.getScale()\r\n        colObj.scale({1, 1, 1.05})\r\n    else\r\n    end\r\nend\r\n\r\nfunction zminus()\r\n    if colObj~=nil then\r\n        a = colObj.getScale()\r\n        colObj.scale({1, 1, 0.95})\r\n    else\r\n    end\r\nend\r\n\r\nfunction onCollisionExit(colInfo) \r\n    colText = \"[b]no collision registered[/b]\\ndrop object here to set source/target.\"\r\n    colObj = nil\r\n    updateDesc()\r\nend\r\n\r\n\r\nfunction updateDesc()\r\n    self.setDescription(colText)\r\nend\n",
